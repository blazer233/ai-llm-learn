{
  "name": "agent2langgraph",
  "version": "1.0.0",
  "description": "Two AI agents communication using LangGraph.js",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "server": "node server/api.js",
    "server:dev": "node --watch server/api.js",
    "client": "cd visualizer && npm run dev",
    "app": "concurrently \"npm run server:dev\" \"npm run client\""
  },
  "keywords": [
    "langgraph",
    "agent",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

{
  "name": "lsb-error-monitor",
  "version": "2.0.0",
  "description": "基于 LSB 隐写技术的前端错误监控方案 - React + Node.js",
  "scripts": {
    "install:all": "cd client && npm install && cd ../server && npm install",
    "start:server": "cd server && npm start",
    "start:client": "cd client && npm run dev",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "decode": "cd server && node decode-cli.js"
  },
  "keywords": [
    "lsb",
    "steganography",
    "error-monitoring",
    "react",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
